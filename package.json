{
	"name": "@appnest/web-config",
	"version": "0.3.34",
	"description": "A Rollup configuration to help you build modern web applications.",
	"author": "Appnest",
	"main": "index.cjs.js",
	"module": "index.esm.js",
	"bugs": {
		"url": "https://github.com/andreasbm/web-config/issues"
	},
	"homepage": "https://github.com/andreasbm/web-config#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/andreasbm/web-config.git"
	},
	"keywords": [
		"configuration",
		"webapp",
		"custom",
		"elements",
		"web",
		"plugins",
		"lit-element",
		"component",
		"rollup",
		"lit-html",
		"template",
		"tslint",
		"typescript",
		"postcss",
		"browserslist",
		"scss",
		"minifying",
		"build",
		"tsconfig"
	],
	"license": "MIT",
	"scripts": {
		"b:dev": "rollup -c --environment NODE_ENV:dev",
		"b:prod": "rollup -c --environment NODE_ENV:prod",
		"s:dev": "rollup -c --watch --environment NODE_ENV:dev",
		"s:prod": "rollup -c --watch --environment NODE_ENV:prod",
		"s": "npm run s:dev",
		"ncu": "ncu -u -a && npm update && npm install",
		"b:lib": "node build.js",
		"bump:patch": "npm version patch && git add . && git commit --no-edit --amend --no-verify",
		"bump:minor": "npm version minor && git add . && git commit --no-edit --amend --no-verify",
		"bump:major": "npm version major && git add . && git commit --no-edit --amend --no-verify",
		"publish:dist": "cd dist && npm publish --access=public && cd ..",
		"publish:patch": "npm run bump:patch && git push && npm run b:lib && npm run publish:dist",
		"publish:minor": "npm run bump:minor && git push && npm run b:lib && npm run publish:dist",
		"publish:major": "npm run bump:major && git push && npm run b:lib && npm run publish:dist",
		"test": "npm run b:lib && karma start karma.conf.js",
		"readme": "node node_modules/.bin/readme"
	},
	"dependencies": {
		"@types/chai": "^4.1.7",
		"@types/mocha": "^5.2.6",
		"@types/node": "^11.11.3",
		"@wessberg/rollup-plugin-ts": "^1.1.34",
		"autoprefixer": "^9.4.10",
		"boxen": "^3.0.0",
		"brotli": "^1.3.2",
		"chai": "^4.2.0",
		"colors": "^1.3.3",
		"cssnano": "^4.1.10",
		"escodegen": "^1.11.1",
		"esprima": "^4.0.1",
		"estraverse": "^4.2.0",
		"filesize": "^4.1.2",
		"fs": "0.0.1-security",
		"fs-extra": "^7.0.1",
		"gzip-size": "^5.0.0",
		"html-minifier": "^3.5.21",
		"karma": "^4.0.1",
		"karma-chai": "^0.1.0",
		"karma-chrome-launcher": "^2.2.0",
		"karma-iframes": "^1.2.2",
		"karma-mocha": "^1.3.0",
		"karma-rollup-preprocessor": "^7.0.0",
		"livereload": "^0.7.0",
		"magic-string": "^0.25.2",
		"minimist": "^1.2.0",
		"mocha": "^6.0.2",
		"node-sass": "^4.11.0",
		"path": "^0.12.7",
		"postcss": "^7.0.14",
		"precss": "^4.0.0",
		"recursive-readdir-sync": "^1.0.6",
		"rollup": "^1.6.0",
		"rollup-plugin-cleaner": "^0.2.0",
		"rollup-plugin-commonjs": "^9.2.1",
		"rollup-plugin-json": "^3.1.0",
		"rollup-plugin-license": "^0.8.1",
		"rollup-plugin-node-resolve": "^4.0.1",
		"rollup-plugin-progress": "^1.0.0",
		"rollup-plugin-serve": "^1.0.1",
		"rollup-plugin-terser": "^4.0.4",
		"rollup-plugin-visualizer": "^1.0.2",
		"rollup-pluginutils": "^2.4.1",
		"targz": "^1.0.1",
		"ts-lit-plugin": "0.1.8",
		"typescript": "^3.3.3333",
		"workbox-build": "^4.1.0"
	},
	"devDependencies": {
		"@appnest/readme": "^1.1.14",
		"lit-element": "^2.0.1",
		"lit-html": "^1.0.0",
		"weightless": "0.0.5"
	},
