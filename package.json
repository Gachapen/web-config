		"@types/escodegen": "0.0.6",
		"@types/esprima": "^4.0.2",
		"@types/estraverse": "0.0.6",
		"@types/filesize": "^5.0.0",
		"@types/fs-extra": "^8.0.1",
		"@types/gzip-size": "^5.1.1",
		"@types/node-sass": "^4.11.0",
		"lit-element": "^2.2.1",
		"lit-html": "^1.1.2",
		"weightless": "0.0.36"
	},
	"contributors": [
